  "dependencies": {
    "bootstrap": "^3.3.7",
    "history": "^4.6.1",
    "marked": "^0.3.6",
    "material-ui": "^0.18.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
